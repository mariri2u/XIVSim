<?xml version="1.0" encoding="utf-8"?>
<ActionAI xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Actions>
    <Action name="フェルクリーヴ">
      <AI group="standard" class="MoreStack" action="インナービースト" value="70" />
      <AI group="standard" class="Require" action="シュトルムブレハ" />
      <AI group="standard" class="Require" action="メイム" />
      <AI group="burst" class="Require" action="原初の解放" />
    </Action>
    <Action name="シュトルムブレハ">
      <AI class="Update" value="10"/>
    </Action>
    <Action name="シュトルムヴィント">
      <AI class="Require" action="シュトルムブレハ" />
    </Action>
    <Action name="メイム">
      <AI class="NoWait" />
    </Action>
    <Action name="ヘビィスウィング">
      <AI class="NoWait" />
    </Action>
    <Action name="アップヒーバル">
      <AI class="NoInterrupt" />
      <AI group="standard" class="Require" action="シュトルムブレハ" />
      <AI group="standard" class="Require" action="メイム" />
      <AI group="standard" class="MoreRecast" action="原初の解放" value="20" />
      <AI group="burst" class="Require" action="原初の解放" />
    </Action>
    <Action name="オンスロート">
      <AI class="NoInterrupt" />
      <AI class="Require" action="原初の解放" />
    </Action>
    <Action name="原初の解放">
      <AI class="NoInterrupt" />
      <AI class="MoreRemain" action="シュトルムブレハ" value="10" />
      <AI class="MoreRemain" action="メイム" value="10" />
    </Action>
    <Action name="ウォークライ">
      <AI class="NoInterrupt" />
      <AI class="LessStack" action="インナービースト" value="50" />
    </Action>
  </Actions>
</ActionAI>